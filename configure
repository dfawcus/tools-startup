  gnustep_flattened=`grep GNUSTEP_FLATTENED=yes $GNUSTEP_MAKEFILES/GNUstep.sh | wc -l | tr -d ' '`
  if test $gnustep_flattened = 1; then
    GNUSTEP_LDIR=$GNUSTEP_LIBRARIES
