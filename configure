fi

if test -n "$ac_init_help"; then
  case $ac_init_help in
     short | recursive ) echo "Configuration of GNUstep Setup 0.19.0:";;
   esac
  cat <<\_ACEOF

Optional Features:
  --disable-FEATURE       do not include FEATURE (same as --enable-FEATURE=no)
  --enable-FEATURE[=ARG]  include FEATURE [ARG=yes]
--enaable-importing-config-file		Enable importing the existing GNUstep configuration file
  --disable-xml			Compile even if XML-dependencies are not met
  --disable-xmltest		Do not try to compile and run a test XML program

